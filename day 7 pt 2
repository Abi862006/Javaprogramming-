  class Student {
    int reg;
    String name;
    Student(int r,String n) {
        reg = r;
        name = n;
    }
    void print() {
        System.out.println(reg +" "+name);
    }
}
public class Main {
    public static void main(String[] args) {
        Student m1 = new Student(101,"alamelu");
        Student m2 = new Student(102,"abi");
        m1.print();
        m2.print();
    }
}
