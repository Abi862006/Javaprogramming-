import java.util.Scanner;
public class EmailInput {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter your email ID: ");
        String email = sc.nextLine();
        System.out.println("Your email ID is: " + email);
    }
}
output:
Enter your email ID: alamelu@gmail.com
Your email ID is: alamelu@gmail.com
8)
public class SubSequenceDemo {
    public static void main(String[] args) {
        String text = "Alamelu";
        CharSequence sub = text.subSequence(0, 6);
        System.out.println("Subsequence: " + sub);
        CharSequence sub2 = text.subSequence(6, text.length());
        System.out.println("Subsequence: " + sub2);
    }
}
output:
Subsequence: Alamel
Subsequence: u
9)
public class SubstringDemo {
    public static void main(String[] args) {
        String text = "Alamelu";
        String sub1 = text.substring(3);
        System.out.println("Substring from index 3: " + sub1);
        String sub2 = text.substring(0, 6);
        System.out.println("Substring from 0 to 6: " + sub2);
    }
}
output:
Substring from index 3: melu
Substring from 0 to 6: Alamel
